{
  "name": "@multilocale/multilocale-js-client",
  "version": "0.0.9",
  "description": "http client for multilocale.com",
  "exports": {
    "./*": {
      "require": "./dist/*",
      "import": "./src/*"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/*js --format cjs",
    "publish": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/multilocale/multilocale-js-client"
  },
  "keywords": [
    "multilocale",
    "multi-language",
    "multilingual",
    "i18n",
    "translate"
  ],
  "author": "Multilocale",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/multilocale/multilocale-js-client/issues"
  },
  "homepage": "https://www.multilocale.com/",
  "dependencies": {
    "@multilocale/check": "0.0.1",
    "b64-lite": "1.4.0",
    "isomorphic-fetch": "2.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/preset-env": "7.20.2",
    "nock": "13.2.9",
    "publint": "^0.1.9",
    "tsup": "^6.5.0"
  }
}
